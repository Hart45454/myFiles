FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Ksl;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    atmosphere
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to4
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to6
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to7
    {
        type            processor;
        value           uniform 0;
    }
}


