FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1623.15;

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 1823.15;
    }
    atmosphere
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
    procBoundary4to0
    {
        type            processor;
        value           uniform 1623.15;
    }
    procBoundary4to2
    {
        type            processor;
        value           uniform 1623.15;
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform 1623.15;
    }
    procBoundary4to6
    {
        type            processor;
        value           uniform 1623.15;
    }
    procBoundary4to7
    {
        type            processor;
        value           uniform 1623.15;
    }
}


