FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      GField;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outlet
    {
        type            calculated;
        value           nonuniform 0();
    }
    inlet
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    atmosphere
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    walls
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    procBoundary7to2
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary7to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary7to4
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary7to5
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary7to6
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


