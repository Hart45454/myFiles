FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "30";
    object      sSmoothField;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    wall
    {
        type            zeroGradient;
    }
    procBoundary6to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary6to2
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary6to4
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary6to7
    {
        type            processor;
        value           uniform 0;
    }
}


