FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "26";
    object      C;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    wall
    {
        type            zeroGradient;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary3to7
    {
        type            processor;
        value           uniform 0;
    }
}


