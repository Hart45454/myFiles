FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "8.5";
    object      surfaceTensionForce;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            calculated;
        value           nonuniform 0();
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    wall
    {
        type            calculated;
        value           uniform 0;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary1to5
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary1to7
    {
        type            processor;
        value           uniform 0;
    }
}


