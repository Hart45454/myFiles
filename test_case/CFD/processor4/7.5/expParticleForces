FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "7.5";
    object      expParticleForces;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    wall
    {
        type            zeroGradient;
    }
    procBoundary4to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary4to6
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


